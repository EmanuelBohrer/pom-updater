        <ns0:dependency>
            <ns0:groupId>org.junit.jupiter</ns0:groupId>
            <ns0:artifactId>junit-jupiter-engine</ns0:artifactId>
            <ns0:version>5.12.0</ns0:version>
            <ns0:scope>test</ns0:scope>
        </ns0:dependency>

        
