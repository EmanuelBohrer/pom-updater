            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-surefire-plugin</ns0:artifactId>
                <ns0:version>2.22.0</ns0:version>
            </ns0:plugin>
        </ns0:plugins>
    </ns0:build>
</ns0:project>